{
  "name": "stylesheet-code-quality-webpack-plugin",
  "version": "0.1.1",
  "description": "A (s)css analysis plugin for webpack",
  "license": "MIT",
  "repository": "florianuphoff/stylesheet-code-quality-webpack-plugin",
  "author": "Florian Uphoff <florianuphoff@mailbox.org>",
  "homepage": "https://github.com/florianuphoff/stylesheet-code-quality-webpack-plugin/",
  "main": "index.js",
  "engines": {
    "node": ">= 12.5.0"
  },
  "scripts": {
    "commitlint": "commitlint",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "eslint --cache src test",
    "ci:lint:commits": "commitlint --from=${CIRCLE_BRANCH} --to=${CIRCLE_SHA1}",
    "lint-staged": "lint-staged",
    "release": "standard-version",
    "release:ci": "conventional-github-releaser -p angular",
    "release:validate": "commitlint --from=$(git describe --tags --abbrev=0) --to=$(git rev-parse HEAD)",
    "security": "nsp check",
    "ci:lint": "npm run lint && npm run security",
    "ci:test": "npm run test -- --runInBand",
    "ci:coverage": "npm run test:coverage -- --runInBand",
    "defaults": "webpack-defaults"
  },
  "files": [
    "lib/",
    "index.js"
  ],
  "keywords": [
    "stylelint",
    "lint",
    "linter",
    "node-sass",
    "webpack",
    "plugin",
    "webpack-plugin"
  ],
  "peerDependencies": {},
  "dependencies": {},
  "devDependencies": {}
}
